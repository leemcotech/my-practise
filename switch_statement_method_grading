
#include <stdio.h>
// switch statement method for coding school grading
    // 80 - 100= A
    // 70 - 79 = B
    // 60 - 69 = C
    // 50 - 59 = D
    // 40 - 49 = E
    // 30 - 39 = F

int main() 
{
    int score;
    
    printf("what is your score:\n");
    
    scanf("%d", &score);
    
    switch(score)
    {
    case 80 ... 100:
    printf("A\n");
    break;
    case 70 ... 79:
    printf("B\n");
    break;
    case 60 ... 69:
    printf("C\n");
    break;
    case 50 ... 59:
     printf("D\n");
     break;
    case 40 ... 49:
    printf("E\n");
    break;
    case 30 ... 39:
    printf("F\n");
     break;
    default:
    printf("invalid scores");
    }
     

    return 0;
}
