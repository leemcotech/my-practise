// Online C compiler to run C program online
#include <stdio.h>

/*
 * a program for school grading system
 * 80 - 100 == A
 * 70 - 79 == B
 * 60 - 69 == C
 * 50 - 59 == D
 * 40 - 49 == E
 * below 40 == F
 * /

int main(void) {
    int score;
    
    printf("what is your score");
    scanf("%d", &score);
    if(score >= 80 && score <=100)
    printf("A");
    else if(score >= 70 && score <=79)
    printf("B");
    else if(score >= 60 && score <=69)
    printf("C");
    else if(score >= 50 && score <=59)
    printf("D");
    else if(score >= 40 && score <=49)
    printf("E");
    else if(score = 40)
    printf("F")
    else
    printf("you have entered an invalid score");
    return 0;
}
